<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/user_management/db" VERSION="20170201" COMMENT="XMLDB file for Moodle local/client"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
  <TABLES>
    <TABLE NAME="local_user_details" COMMENT="User organization details">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="salutation" TYPE="char" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="" SEQUENCE="false"/>
        <FIELD NAME="type" TYPE="char" LENGTH="50" NOTNULL="false" UNSIGNED="true" DEFAULT="" SEQUENCE="false"/>
        <FIELD NAME="category" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" />
        <FIELD NAME="dob" TYPE="char" LENGTH="50" NOTNULL="false" UNSIGNED="true" DEFAULT="" SEQUENCE="false"/>
        <FIELD NAME="doj" TYPE="char" LENGTH="50" NOTNULL="false" UNSIGNED="true" DEFAULT="" SEQUENCE="false"/>
        <FIELD NAME="jobtype" TYPE="char" LENGTH="100" NOTNULL="false" UNSIGNED="true" DEFAULT="" SEQUENCE="false"/>
        <FIELD NAME="grade" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" />
        <FIELD NAME="post" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" />
        <FIELD NAME="caste" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" />
        <FIELD NAME="gender" TYPE="char" LENGTH="50" NOTNULL="false" UNSIGNED="true" DEFAULT="" SEQUENCE="false"/>
        <FIELD NAME="department" TYPE="char" LENGTH="255" NOTNULL="false" UNSIGNED="true" DEFAULT="" SEQUENCE="false" />
        <FIELD NAME="position" TYPE="char" LENGTH="255" NOTNULL="false" UNSIGNED="true" DEFAULT="" SEQUENCE="false" />
        <FIELD NAME="address" TYPE="char" NOTNULL="false" UNSIGNED="true" DEFAULT="" SEQUENCE="false" />
        <FIELD NAME="uid" TYPE="int" LENGTH="20" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" />
        <FIELD NAME="schoolid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" />
        <FIELD NAME="subject" TYPE="char" LENGTH="255" NOTNULL="false" UNSIGNED="true" DEFAULT="" SEQUENCE="false" />
        <FIELD NAME="deleted" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" DEFAULT="0" />
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" DEFAULT="0" />
        <FIELD NAME="usercreated" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" />
        <FIELD NAME="usermodified" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="userdeleted" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="id" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

  </TABLES>
</XMLDB>
