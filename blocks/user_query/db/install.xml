<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/user_query/db" VERSION="2023051000" COMMENT="XMLDB file for Moodle user query"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_user_query" COMMENT="user query details">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" />
        <FIELD NAME="user_email" TYPE="char" LENGTH="150" NOTNULL="true" SEQUENCE="false" />
        <FIELD NAME="user_query" TYPE="text" NOTNULL="true" SEQUENCE="false" />
         <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="id" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
